<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="shop" match-name="customer"/>
  <table-filter match-catalog="shop" match-name="order"/>
  <table-filter match-catalog="shop" match-name="order_detail"/>
  <table-filter match-catalog="shop" match-name="product"/>
